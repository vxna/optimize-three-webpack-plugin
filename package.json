{
  "name": "@vxna/optimize-three-webpack-plugin",
  "version": "0.0.0-development",
  "description": "A compat layer that enables tree shaking and human-readable imports",
  "author": "vxna",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vxna/optimize-three-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "three"
  ],
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "pretest": "yarn lint",
    "lint": "eslint src/**/*.js --format codeframe --fix",
    "test": "cross-env NODE_ENV=production jest",
    "format": "prettier src/**/*.{js,md} --write",
    "posttest": "yarn format"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.1",
    "memfs": "^3.1.2",
    "memory-fs": "^0.5.0",
    "prettier": "^2.0.2",
    "three": "^0.115.0",
    "webpack": "^4.42.1"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier"
    ],
    "env": {
      "jest": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{js,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
